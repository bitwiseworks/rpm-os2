    sys.stdout.write('%s\n' % msg)

def ocb(what, amount, total, key, data):
    print(what, amount, total, type(key), data)
    return -1

def ncb(arg, what, amount, total, data):
    print(type(arg), what, amount, total, data)
    return -1

