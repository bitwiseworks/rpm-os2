    (('a', '=', '1.2-1'),	('a', '<>', '1.2'),	0),

    (('a', '>', '1.2-1'),	('a', '=', '1.2'),	1),
    (('a', '>', '1.2-1'),	('a', '>=', '1.2'),	1),
    (('a', '>', '1.2-1'),	('a', '<=', '1.2'),	1),
