{ set +x
$as_echo "$at_srcdir/rpmvfylevel.at:77:


if ! [ -d testing/ ]; then
    cp -aP \"\${RPMTEST}\" .
    chmod -R u+w testing/
    mkdir -p testing/build
    ln -s ../data/SOURCES testing/build/
fi
