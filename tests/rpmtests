   conv=notrunc bs=1 seek=7711 count=6 2> /dev/null

echo "INSTALL 1"
runroot rpm -U --ignorearch --ignoreos --nodeps \
	--define "_pkgverify_level none" \
	/tmp/${pkg} 2>&1

echo "INSTALL 2"
runroot rpm -U --ignorearch --ignoreos --nodeps --nosignature \
	--define "_pkgverify_level none" \
	/tmp/${pkg} 2>&1

echo "INSTALL 3"
runroot rpm -U --ignorearch --ignoreos --nodeps \
	--define "_pkgverify_level signature" \
