  --eval '%{lua: v1 = rpm.ver(\"4.16\"); print(v1, v1.e, v1.v, v1.r)}' \\
  --eval '%{lua: v1 = rpm.ver(\"1:4.16-5\"); v2 = rpm.ver(\"4.4-1\"); print(v1 < v2, v1 == v2, v1 > v2, v1 == v1)}'
